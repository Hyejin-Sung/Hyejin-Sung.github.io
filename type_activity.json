{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/Hyejin-Sung/Hyejin-Sung.github.io/refs/heads/main/type_activity.csv"
  },
  "title": {
    "text": "Employment, Unemployment, and Economic Inactivity",
    "fontSize": 10,
    "subtitle": "Employment, Unemployment, and Economic Inactivity for people aged 16 and over (1971-2004)",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "width": 300,
  "height": 300,
  "layer": [
    {
      "mark": {
        "type": "line",
        "strokeWidth": 2,
        "color": "#3498db"
      },
      "encoding": {
        "x": {
          "field": "Year",
          "type": "temporal",
          "axis": {
            "grid": false
          }
        },
        "y": {
          "field": "Employment",
          "type": "quantitative",
          "axis": {
            "title": "Percentage",
            "grid": true
          }
        }
      }
    },
    {
      "mark": {
        "type": "line",
        "strokeWidth": 2,
        "color": "#FF5733"
      },
      "encoding": {
        "x": {
          "field": "Year",
          "type": "temporal"
        },
        "y": {
          "field": "Unemployment",
          "type": "quantitative"
        }
      }
    },
    {
      "mark": {
        "type": "line",
        "strokeWidth": 2,
        "color": "#2ecc71"
      },
      "encoding": {
        "x": {
          "field": "Year",
          "type": "temporal"
        },
        "y": {
          "field": "Inactivity",
          "type": "quantitative"
        }
      }
    }
  ]
}
